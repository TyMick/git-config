[checkout]
  defaultRemote = origin

[core]
  editor = code-insiders --wait

[diff]
  tool = vscode

[difftool "vscode"]
  cmd = code-insiders --wait --diff $LOCAL $REMOTE

[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true

[init]
  defaultBranch = main

[merge]
  tool = vscode

[mergetool "vscode"]
  cmd = code-insiders --wait $MERGED

[pull]
  ff = only

[user]
  name = Ty Mick
  useConfigOnly = true
